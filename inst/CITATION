citHeader("To cite SemNeT in publications use:")

citEntry(entry = "Manual",
  title        = "{SemNeT}: Methods and measure for semantic network analysis",
  author       = personList(as.person("Alexander P. Christensen","Yoed N. Kenett")),
  year         = "2018",
  url          = "Retrieved from github.com/AlexChristensen/SemNeT",

  textVersion  =
  paste("Christensen, A. P., & Kenett, Y. N. (2018).",
        "SemNeT: Methods and measure for semantic network analysis.",
        "R package version 0.1.0.",
        "Retrieved from github.com/AlexChristensen/SemNeT")
)